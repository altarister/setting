<div class="memebox-menu-wrap"
     data-component="menu"
     data-component-data='{{{json component.menu}}}'>
    <ul class="memebox-menu">
        {{#each component.menu}}
            <li>
                <a href="{{this.url}}">
                    {{this.text}}
                </a>
            </li>
        {{/each}}
    </ul>
</div>